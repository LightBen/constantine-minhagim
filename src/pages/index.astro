<meta http-equiv="refresh" content="0;url=/fr/" />
<script>
    // Client-side redirect based on user language preference
    const userLang = navigator.language;
    if (userLang.startsWith("he")) {
        window.location.href = "/he/";
    } else {
        window.location.href = "/fr/";
    }
</script>
